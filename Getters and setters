#getters and setters 
class MYClass:
  def __init__(self):
    self._value=ValueError
  def get_value(self):
    return self._value
  def set_value(self,value): # Fixed: Changed 'set_values' to 'set_value'
    self._value=value
obj1=MYClass()
obj1.set_value(10) # Fixed: Changed 'set_values' to 'set_value'
print(obj1.get_value())
