#demonstrate polymorphism 
class shape:
  def area(self):
    pass
class circle(shape):
  def __init__(self,radius):
    self.radius=radius
  def area(self):
    return 3.14*self.radius*self.radius
class circle(shape):
  def __init__(self,radius):
    self.radius=radius
  def area(self):
    return 3.14*self.radius*self.radius
circle1=circle(5)
print(circle1.area())

obj1=circle(5)
print(obj1.area())
